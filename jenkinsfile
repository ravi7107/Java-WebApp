pipeline {
    agent { label "build_server" }

    stages {
        stage("This stage will build the package") {
            steps {
                sh "mvn clean"
                sh "mvn install"
            }
        }   
        stage("This stage will copy package to S3"){
            steps{
                sh "aws s3 cp /home/ubuntu/build_server/workspace/Job_3/target/WebApp.war s3://mys301082023" 
                }
            }

        }
    }
